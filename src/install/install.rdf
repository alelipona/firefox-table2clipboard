<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>@UUID@</em:id>
    <em:name>@EXT_TITLE_NAME@</em:name>
    <em:version>@VERSION@</em:version>
    <em:description>Allow to copy to clipboard an HTML table rows/columns selection correctly formatted</em:description>
    <em:creator>Davide Ficano</em:creator>

    <em:translator>nirwana (Dutch translation)</em:translator>
    <em:translator>La Luciole (French translation)</em:translator>
    <em:translator>urko (Spanish translation)</em:translator>
    <em:translator>Oliver Roth "ReinekeFux" (German translation)</em:translator>
    <em:translator>damian (Polish translation)</em:translator>
    <em:translator>Batuhan &#199;etin (Turkish translation)</em:translator>
    <em:translator>TLemur (Russian translation)</em:translator>
    <em:translator>rickcart (Chinese translation)</em:translator>
    <em:translator>SlovakSoft (Slovak translation)</em:translator>
    <em:translator>teboga (Brazilian translation)</em:translator>
    <em:translator>Emil Stanchev (Bulgarian translation)</em:translator>
    <em:translator>AlleyKat (Danish translation)</em:translator>
    <em:translator>Pedram Veisi (Persian translation)</em:translator>
    <em:translator>Jan Bruusgaard (Norwegian translation)</em:translator>

    <em:homepageURL>http://dafizilla.sourceforge.net/table2clip</em:homepageURL>

    <!-- Front End Integration Hooks (used by Extension Manager)-->
    <em:optionsURL>chrome://@EXT_NAME@/content/settings/settings.xul</em:optionsURL>
    <em:iconURL>chrome://@EXT_NAME@/skin/appicon.png</em:iconURL>

    <em:updateURL>@UPDATE_URL@</em:updateURL>

    <em:file>
      <Description about="urn:mozilla:extension:file:@EXT_NAME@.jar">
        <em:package>content/@EXT_NAME@/</em:package>
        <em:locale>locale/en-US/@EXT_NAME@/</em:locale>
        <em:locale>locale/it-IT/@EXT_NAME@/</em:locale>
        <em:locale>locale/de-DE/@EXT_NAME@/</em:locale>
        <em:locale>locale/es-ES/@EXT_NAME@/</em:locale>
        <em:locale>locale/fr-FR/@EXT_NAME@/</em:locale>
        <em:locale>locale/nl-NL/@EXT_NAME@/</em:locale>
        <em:locale>locale/pl-PL/@EXT_NAME@/</em:locale>
        <em:locale>locale/tr-TR/@EXT_NAME@/</em:locale>
        <em:locale>locale/ru-RU/@EXT_NAME@/</em:locale>
        <em:locale>locale/zh-CN/@EXT_NAME@/</em:locale>
        <em:locale>locale/sk-SK/@EXT_NAME@/</em:locale>
        <em:locale>locale/pt-BR/@EXT_NAME@/</em:locale>
        <em:locale>locale/bg-BG/@EXT_NAME@/</em:locale>
        <em:locale>locale/da-DK/@EXT_NAME@/</em:locale>
        <em:locale>locale/fa-IR/@EXT_NAME@/</em:locale>
        <em:locale>locale/nb-NO/@EXT_NAME@/</em:locale>
        <em:skin>skin/classic/@EXT_NAME@/</em:skin>
      </Description>
    </em:file>

    <!-- FireFox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>1.0</em:minVersion>
        <em:maxVersion>3.0a7</em:maxVersion>
      </Description>
    </em:targetApplication>

